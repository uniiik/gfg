#include <iostream>
using namespace std;

int main() {

int n;
cin>>n;
while(n>0)
{
    int a;
    cin>>a;
    int array[a];
    for(int i=0;i<a;i++)
    {
        cin>>array[i];
    }
    for(int j=a-1;j>=0;j--)
    {
        cout<<array[j]<<" ";
        
    }cout<<endl;
    
    
    
    
    
    
    n--;
}
}
